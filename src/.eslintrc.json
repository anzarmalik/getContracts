{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true
        }
      ],
      "linebreak-style": "off",
      "no-multi-assign": "off",
      "no-console": "off",
      "eqeqeq": "off",
      "no-unused-expressions": "warn",
      "no-var": "warn"
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"]
  }